<div>
  <div>
    <div>
      <h3>User info</h3>
      <%= image_tag user.get_profile_image(100,100) %>
      <label for="user_name">Name</label>
      <p><%= user.name %></p>
      <p>introduction</p>
      <p><%= user.text %></p>
      <% if user == current_user %>
        <p><%= link_to "User info", edit_user_path(user) %></p>
      <% end %>
       <h1>New book</h1>
      <%= form_with model: @booked do |f| %>
      <h4>Title</h4>
      <%= f.text_field :title %>
      <h4>Opinion</h4>
      <%= f.text_area :body %><br>
      <%= f.submit 'Create Book' %>
      <% end %>
    </div>
  </div>
</div>